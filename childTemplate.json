{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "functionAppName": {
            "type": "string"
        }
    },
    "variables": {
        "sitesWebApiVersion": "2016-08-01"
    },
    "resources": [],
    "outputs": {
        "configtriggerUrl": {
            "type": "string",
            "value": "[listsecrets(resourceId('Microsoft.Web/sites/functions', parameters('functionAppName'), 'GetServiceDetailsHttpTrigger'), variables('sitesWebApiVersion')).trigger_url]"
        },
        "traintriggerUrl": {
            "type": "string",
            "value": "[listsecrets(resourceId('Microsoft.Web/sites/functions', parameters('functionAppName'), 'cutterFunction'), variables('sitesWebApiVersion')).trigger_url]"
        },
        "datasetUploadCallback": {
            "type": "string",
            "value": "[listsecrets(resourceId('Microsoft.Web/sites/functions', parameters('functionAppName'), 'DataUploadCompleteHttpTrigger'), variables('sitesWebApiVersion')).trigger_url]"
        }

    }
}